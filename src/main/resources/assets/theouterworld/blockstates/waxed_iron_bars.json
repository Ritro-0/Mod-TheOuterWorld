{
  "multipart": [
    {
      "apply": { "model": "minecraft:block/iron_bars_post_ends" }
    },
    {
      "when": { "north": false, "east": false, "south": false, "west": false },
      "apply": { "model": "minecraft:block/iron_bars_post" }
    },
    {
      "when": { "north": true, "east": false, "south": false, "west": false },
      "apply": { "model": "minecraft:block/iron_bars_cap" }
    },
    {
      "when": { "north": false, "east": false, "south": false, "west": true },
      "apply": { "model": "minecraft:block/iron_bars_cap", "y": 90 }
    },
    {
      "when": { "north": false, "east": false, "south": true, "west": false },
      "apply": { "model": "minecraft:block/iron_bars_cap", "y": 180 }
    },
    {
      "when": { "north": false, "east": true, "south": false, "west": false },
      "apply": { "model": "minecraft:block/iron_bars_cap", "y": 270 }
    },
    {
      "when": { "north": true },
      "apply": { "model": "minecraft:block/iron_bars_side" }
    },
    {
      "when": { "east": true },
      "apply": { "model": "minecraft:block/iron_bars_side", "y": 90 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "minecraft:block/iron_bars_side_alt" }
    },
    {
      "when": { "west": true },
      "apply": { "model": "minecraft:block/iron_bars_side_alt", "y": 90 }
    }
  ]
}

